/* harmony default export */ var multi_selectvue_type_script_lang_js_ = ({
  props: {
    ms_options: {
      type: Array
    },
    name: '',
    placeholder: '',
    ms_options_selected_values: {
      type: Array,
      default: function _default() {
        return [];
      }
    },
    ms_options_selected_labels: {
      type: Array,
      default: function _default() {
        return [];
      }
    }
  },
  computed: multi_selectvue_type_script_lang_js_objectSpread({}, Object(vuex_esm["b" /* mapState */])({
    user: function user(state) {
      return state.localStorage.user;
    }
  })),
  methods: {
    //Add multiselect option to input when selected, and show it in the multiselect
    selectMultiselectOption: function selectMultiselectOption(option) {
      //Emit this data to parent (so value selected/deselected can be accessed in the parent)
      this.$emit('selectMultiselectOption', option.getAttribute('data-value')); //If option is not active

      if (!option.classList.contains('select--multi__list__option--active')) {
        //add to options_selected props
        this.ms_options_selected_values.push(option.getAttribute('data-value'));
        this.ms_options_selected_labels.push(option.innerHTML); //add active class

        option.classList.add('select--multi__list__option--active');
      } else {
        //remove from options_selected props
        this.ms_options_selected_values.pop(option.getAttribute('data-value'));
        this.ms_options_selected_labels.pop(option.innerHTML); //remove active class

        option.classList.remove('select--multi__list__option--active');
      }
    }
  }
});
