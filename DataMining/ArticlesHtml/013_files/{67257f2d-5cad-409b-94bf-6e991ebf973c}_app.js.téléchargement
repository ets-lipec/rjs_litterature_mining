        eloquaPostParameters += this.$store.state.signin.modal_new_user_extra_eloqua_params;
      } else {
        eloquaPostParameters += '&emailAddress=' + this.email_address;
      }

      console.log(eloquaPostParameters); //Post data to Eloqua

      axios_default.a.post(this.eloqua.formaction_new_user + '?' + eloquaPostParameters).then(function (response) {
        //Hide loading spinner
        document.getElementById('createProfileButtonLoadingSpinner').style.display = 'none';

        _this3.$store.commit('signin/updateSigninModalSection', 'not-exists-success');
      });
    }
  },
  data: function data() {
    return {
      email_address: '',
      first_name: '',
      last_name: '',
      country: '',
      audience_types: [],
      selected_audience_types: []
    };
  }
});
