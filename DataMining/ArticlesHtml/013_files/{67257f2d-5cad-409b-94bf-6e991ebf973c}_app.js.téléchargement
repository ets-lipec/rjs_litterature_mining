      this.$store.commit('localStorage/updateUserClosedBetterRecModal', true);
    },
    //Update users selected audience types
    updateSelectedAudienceTypes: function updateSelectedAudienceTypes(option) {
      //If option selected is already in selected_audience_types, remove it...else add it
      if (this.selected_audience_types.includes(option)) {
        this.selected_audience_types.pop(option);
      } else {
        this.selected_audience_types.push(option);
      }
    },
    //Create users profile if they've tried to sign in but don't have a profile
    userUpdateWhatyoudo: function userUpdateWhatyoudo(event) {
      if (event) event.preventDefault();
