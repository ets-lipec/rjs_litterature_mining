vue_runtime_esm["a" /* default */].mixin({
  methods: {
    //Store users last interacted page url (if signed out) and then redirect them to that when they sign in
    redirectUserToLastVisitedPage: function redirectUserToLastVisitedPage() {
      if (this.$store.state.localStorage.user.status === 'signedout') {
        this.$store.commit('localStorage/updateUserLastVisitedPageUrl', $nuxt.$route.path);
      }
    },
    //Sign user in (when pcl is passed)
    signUserIn: function signUserIn() {
      //Set var for store 'user' value
      var user = this.$store.state.localStorage.user,
      userData = {}; //If on dev, set users data to to that in the js data object

      if ( false || window.location.href.indexOf("acs.evensix") > -1) {
		userData = {}; //If on dev, set users data to to that in the js data object
      } //else get data from Eloqua field merge
      else {
          userData = eloquaUserData;
        } //If user is not signed in and pcl query param exists


      if (user.status !== 'signedin' && $nuxt.$route.query.pcl) {
        this.$store.commit('localStorage/updateUserVisistedAppWithPclParam', true);
      } //if the user has a session, the current time is within their expiration time and they have an email in their local storage
      // if(user.visited_app_with_pcl_param && (new Date() < new Date(user.expiration_time)) && user.data.emailaddress && $nuxt.$route.query.pcl)
      // {
      // }
      //If pcl param is passed


      if ($nuxt.$route.query.pcl && userData.emailaddress) {
        //Sign user in
        this.$store.commit('localStorage/updateUserStatus', 'signedin'); //Set users data

        this.$store.commit('localStorage/setUserData', userData); //Send signed in data layer to Tag Manager

        this.$gtm.pushEvent({
          event: 'visitorLog',
          Signed_In_Status: 'Signed In'
        });
        this.syncLiteratumData(userData);
        this.clearUsersFollowedJournals(); //Strip pcl param from url

        if ($nuxt.$route.query.pcl) {
          window.history.replaceState({}, document.title, window.location.href.replace('?pcl=' + $nuxt.$route.query.pcl + '&', '?').replace('?pcl=' + $nuxt.$route.query.pcl, ''));
        }
      }
    },
    //Sign user out
    signUserOut: function signUserOut() {
      //Sign user out
      this.$store.commit('localStorage/updateUserStatus', 'signedout'); //Clear users data

      this.$store.commit('localStorage/setUserData', {}); //Delete eloqua cookie

      document.cookie = "ELOQUA=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
    },
    updateUserExpiration: function updateUserExpiration() {
      if (this.$store.state.localStorage.user.status === 'signedin') {
        // console.log(new Date() + ' - current time');
        // console.log(new Date(this.$store.state.localStorage.user.expiration_time) + ' - expire');
        //If user hasn't interacted with the app before their expiration time, sign them out and show the sign in modal
        if (new Date(this.$store.state.localStorage.user.expiration_time) < new Date()) {
          // console.log('user is signed out, show sign in modal');
          this.$store.commit('localStorage/updateUserStatus', 'signedout');
          this.$store.commit('signin/toggleSigninModal', {
            show_modal: true
          });
        } //Update user expiration time


        Date.prototype.addMinutes = function (minutes) {
          this.setMinutes(this.getMinutes() + minutes);
          return this;
        };

        this.$store.commit('localStorage/updateUserExpiration', new Date().addMinutes(120));
      }
    }
  }
});
